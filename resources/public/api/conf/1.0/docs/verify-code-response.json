{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Email verification - Verify Code Response",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "examples": [
        "CODE_VERIFIED"
      ]
    },
    "message": {
      "type": "string",
      "examples": [
        "The verification code for the email verified successfully"
      ]
    }
  },
  "required": [
    "status",
    "message"
  ]
}

